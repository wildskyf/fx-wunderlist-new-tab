{
  "applications": {
    "gecko": {
      "id": "@wlisttab",
      "strict_min_version": "54.0a1"
    }
  },
  "manifest_version": 2,
  "name": "Wunderlist New Tab",
  "version": "2.4.0",

  "description":
    "This addon would replace your boring new-tab page to a good-designed wunderlist page.",
  "homepage_url": "https://github.com/wildskyf/fx-wunderlist-new-tab",
  "icons": {
    "48": "icons/border-48.png"
  },

  "chrome_url_overrides": {
    "newtab": "data/index.html"
  },

  "permissions": [
      "*://a.wunderlist.com/*", "*://socket.wunderlist.com:8443/*",
      "tabs", "storage", "identity"
  ],

  "background": {
    "scripts": ["index.js"]
  }
}
